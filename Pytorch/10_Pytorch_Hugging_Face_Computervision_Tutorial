l"}},{"cell_type":"code","source":["from datasets import load_dataset\n","\n","base_url = \"https://github.com/crux82/squad-it/raw/master/\"\n","data_files = {\n","    \"train\": base_url + \"SQuAD_it-train.json.gz\",\n","    \"test\": base_url + \"SQuAD_it-test.json.gz\"\n","}\n","squad_it_dataset = load_dataset(\"json\", data_files=data_files, field=\"data\")\n","squad_it_dataset"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":319,"referenced_widgets":["f589ba9680ba4caaad03bf60229117d0","6802db74b2d24147b039dae1fe722391","1d7b502780cd4b2683ced364edc79aeb","67cfdfcd1c104cd495ec0347fc5e5881","a0b9904bd0cb4946b8eeb8295bc67ae0","02429f6010474a42b8e0f115a9e352c6","40e992417d274ad78631cd611d06595a","e40414d677794633991dcfd75f0a7e90","9195952cc5ea42c1a29b0733c6886fd0","c67af0b2314c4276ac90043f8840ec89","7424f7f699da418babd8495a53dcbc0f","638d1946a9704ef39ed837202afcd327","12ac5427615c44fc96e120d4d528d42a","9a793a34f7174b4dae89cfa1d7bf3c8b","b16e6f36704d4942bf1013b20eeeb4bb","7681602333904c19b201d947fda37d9e","6b6243598f994db89942475921ba6a6f","43b2188eb7f441dba5cca11c3bc464f9","c751137561734e02a666424fd57fdad5","8aaef20fa39f490aa4a22d24ff2a9bc6","d8639c16cdd94f3ba9e8d6e512b79bfa","71317cdfb0244da3835bb2108e30e5d4","c071e5527869433e84c2821c44f50962","cdc76c556cfd41958b9e359c71427a99","dfb44beb17014549a31776ef28f449e1","5b8e92e29d4c4489bc8838e1df73b4e1","dfa05676f3d24e1c91b1d41ca690f6af","e8e9db50edc7463da9e46bc16626a2d4","a0901e5aed844390ab00b2942a41a74c","2ae8fbe76f504e2490670915e92331e7","013ff4560d884ad9b311a4ee00351bbd","3313483b3d604fa7b4f09da5df36fbf4","664f28481ac44dd599c1931bba5b11cb","bb8c6abd456d45e9b2c8211762f86e0f","fe08ed0e376d434a848af18952f3a62c","c495a11d99054c33adc255d404d32fb0","2456c9694f2341fc8c4802dda88478c0","b9a9c53b9d1d484792af3ec4c06df885","ee63e1a6976e416c9d4fd3db47627cb4","761d7224cee64228b83bb1967c41ae19","c039febaa0e9458683bcbe2b4d591d68","447c312bc47c475ab0052013db0a9e12","7d8facd2de9241c9828e9d225a54f409","3f832fd0067742