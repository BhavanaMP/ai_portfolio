      ],
      "source": [
        "#Now can save the model and the tokenizer in the local-folder where we clone the repo\n",
        "\n",
        "model.save_pretrained(repo.local_dir)\n",
        "tokenizer.save_pretrained(repo.local_dir)"
      ]
    },
    {
      "cell_type": "code",
