      },
      "outputs": [],
      "source": [
        "#push it to repository where huggingface uses git-lfs under the hood without any configs needed\n",
        "\n",
        "repo.git_add() #staging\n",
        "repo.git_commit(commit_message=\"Added models config, params and tokenizer related files to repo\")\n",
        "repo.git_push()"
      ]
    },
